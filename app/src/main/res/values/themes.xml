<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="Theme.newjeansBunnies" parent="Theme.AppCompat.DayNight.NoActionBar" >
        <item name="android:windowSplashScreenBackground">@color/white</item>

    </style>

    <style name="Theme.newjeansBunnies.TransparentSystemBar">
        <item name="android:statusBarColor">@android:color/transparent</item>
        <item name="android:navigationBarColor">@android:color/transparent</item>
    </style>

    <style name="Theme.newjeansBunnies.Start" parent="@android:style/Theme.Material">
        <item name="android:windowNoTitle">true</item>
        <item name="android:colorBackground">@android:color/white</item>
        <item name="android:windowBackground">@android:color/white</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:navigationBarColor">#000000</item>
    </style>

</resources>
