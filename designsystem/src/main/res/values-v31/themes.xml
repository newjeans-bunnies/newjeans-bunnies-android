<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="Theme.Newjeansbunnies" parent="Theme.AppCompat.NoActionBar">
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:background">@color/white</item>
    </style>

    <style name="Theme.Newjeansbunnies.Starting" parent="Theme.AppCompat.NoActionBar">
        <item name="android:windowSplashScreenBackground">@color/white</item>
        <item name="android:windowSplashScreenAnimatedIcon">@drawable/ic_splash</item>
        <item name="android:statusBarColor">@color/white</item>
        <item name="android:navigationBarColor">@color/white</item>
        <item name="postSplashScreenTheme">@style/Theme.Newjeansbunnies</item>
        <item name="splashScreenIconSize">288dp</item>
    </style>

</resources>